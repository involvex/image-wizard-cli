{
  "name": "@involvex/image-wizard-cli",
  "version": "0.0.1",
  "description": "Cli to generate ai images with bing",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/involvex"
  },
  "type": "module",
  "bin": {
    "image-wizard": "dist/index.js"
  },
  "scripts": {
    "prebuild": "bun run format && bun run lint:fix && bun run typecheck",
    "build": "bun build --target node --outdir dist --entry src/index.ts",
    "dev": "bun run ./src/index.ts",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "bun run ./dist/index.js",
    "pretest": "bun run format && bun run lint:fix && bun run typecheck",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/involvex/image-wizard-cli.git"
  },
  "author": "involvex",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "1.0.0",
    "@google/genai": "1.39.0",
    "@types/fs-extra": "11.0.4",
    "@types/mime": "4.0.0",
    "commander": "14.0.3",
    "conf": "15.0.2",
    "dotenv": "17.2.3",
    "fs-extra": "11.3.3",
    "mime": "4.1.0",
    "picocolors": "1.1.1"
  },
  "devDependencies": {
    "@types/node": "25.1.0",
    "eslint": "9.39.2",
    "jiti": "2.6.1",
    "prettier": "3.8.1",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-packagejson": "3.0.0",
    "prettier-plugin-sort-imports": "1.8.9",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0"
  }
}